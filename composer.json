{
    "name": "wpapp/user-tables",
    "description": "This plugin displays users in a dynamic table.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Oleh Kosarenko",
            "email": "kosarenko.oleh@gmail.com"
        }
    ],
    "support": {
        "src": "https://github.com/olehkosarenko/user-tables",
        "issues": "https://github.com/olehkosarenko/user-tables/issues"
    },
    "require": {
        "php": ">=7.2",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "WpApp\\UserAtlas\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
